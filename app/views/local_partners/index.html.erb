<p id="notice"><%= notice %></p>

<h1>Listing Local Partners</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @local_partners.each do |local_partner| %>
      <tr>
        <td><%= link_to 'Show', local_partner %></td>
        <td><%= link_to 'Edit', edit_local_partner_path(local_partner) %></td>
        <td><%= link_to 'Destroy', local_partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Local partner', new_local_partner_path %>
