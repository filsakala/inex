<p id="notice"><%= notice %></p>

<h1>Listing Event Table Columns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_table_columns.each do |event_table_column| %>
      <tr>
        <td><%= link_to 'Show', event_table_column %></td>
        <td><%= link_to 'Edit', edit_event_table_column_path(event_table_column) %></td>
        <td><%= link_to 'Destroy', event_table_column, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

